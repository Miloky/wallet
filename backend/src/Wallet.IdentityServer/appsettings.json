{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "react_auth": {
        "Profile": "SPA",
        "RedirectUri": "http://localhost:3000/login-callback",
        "LogoutUri":"http://localhost:3000/logout-callback",
        "AllowedGrantTypes": "code",
        "RequirePkce":true
      }
    }
  },
  "AllowedHosts": "*"
}
